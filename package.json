  "description": "A library of UI components for React",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "rm -rf lib && babel src -d lib && npm run build:sass",
    "build:sass": "gulp rename",
    "build:base64": "gulp base64",
    "build:dist": "gulp dist",
